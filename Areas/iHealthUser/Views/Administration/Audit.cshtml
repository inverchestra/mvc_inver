@using InnDocs.iHealth.Web.UI.Models
@model LoginAndSignUp
@if (Model.Audits.Count > 0)
{
    <table id="pra">
        <tbody>
            @foreach (var audits in Model.Audits)
            {
                <tr>
                    <td style="width: 16%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;">@audits.Name
                    </td>
                    <td style="width: 15%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;">@audits.Module
                    </td>
                    <td style="width: 18%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;">@audits.AuditEvent
                    </td>
                    <td style="width: 12%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;">@audits.CaptureMode
                    </td>
                    <td style="width: 16%; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;">@audits.AuditDate
                    </td>
                </tr>
            }
            
        </tbody>
    </table>
}
else
{
    <text>
    <center>
        ----------- No Reports to display -----------
    </center></text>
}
<div id="UserAuditsDiv">
</div>
