@model InnDocs.iHealth.Web.UI.Models.PromoCode
<link href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" rel="stylesheet" />
<script type="text/javascript">
    $('#tbExpireOn').datepicker({

        changeYear: true,
        changeMonth: true,
        minDate: 0,
        onSelect: function () {
            //            UpdateStartDate();
        }
    });


</script>
@Scripts.Render("~/bundles/PCode")
<div id="AddUserDiv">
    @*<form id="register-form" action="@Url.Action("SubmitPCode", "Administration")">*@
    @* @using (Html.BeginForm("SubmitPCode", "Administration", new { @class = "crtproc" }))
{*@
    @using (Ajax.BeginForm("SubmitPCode", "Administration", new AjaxOptions() { HttpMethod = "POST", OnSuccess = "closeModal", OnFailure = "displayError", OnBegin = "loading" }, new { @class = "regAdmin" }))
    {
        @Html.ValidationSummary(true)
        <div id="register">
            <div id="rhead" style="border-bottom: 1px solid rgb(218,221,226);">
                <h3>
                    Create Promo Code</h3>
            </div>
            <div id="rbody" class="Payment">
                <ul>
                    <li class="labelnormal">Promo Code<text style="color: Red">*</text></li>
                    <li class="payment_txtbx">@Html.TextBoxFor(model => model.PromoCodes, new { @placeholder = "Enter Promo code", title = "Enter your Promo code", @class = "logintxts" })
                        @Html.ValidationMessageFor(model => model.PromoCodes)</li>
                    <li class="labelnormal">Group Name<text style="color: Red">*</text></li>
                    <li class="payment_txtbx">@Html.TextBoxFor(model => model.CodeUser, new { @placeholder = "Enter Group Name", title = "Enter Group Name", @class = "logintxts" })
                        @Html.ValidationMessageFor(model => model.CodeUser)</li>
                    <li class="labelnormal">Expire Date<text style="color: Red">*</text></li>
                    <li class="payment_txtbx">@Html.TextBoxFor(model => model.ExpireOn, new { @placeholder = "Enter Expire Date", title = "Enter Expire Date", @class = "logintxts", id = "tbExpireOn" })
                        @Html.ValidationMessageFor(model => model.ExpireOn)</li>
                    <li class="labelnormal"></li>
                    <li class="payment_txtbx">
                        <div style="width: 100%; float: right; margin: 0 7px 0 0;" class="inline">
                            @*<input id="btncancel" type="button" value="cancel" style="float: right" class="graygrad" />*@
                            <input type="button" class="graygrad" value="close" onclick="$.modal.close();" />
                            <input id="btnsubmit" type="submit" value="submit" class="greengrad" />
                            @*<input type="submit" id="btnSubmit" name="save" value="Save" class="greengrad" />
<input type="button" name="cancel" value="Cancel" class="graygrad" />*@
                        </div>
                    </li>
                </ul>
            </div>
        </div>
@*</form>*@
    }
</div>
