                                            }
                                        </li>
                                        <li class="labelChart">Date Of Chart: </li>
                                        <li class="labelChartB">@chart.ChartDate
                                        </li>
                                        <li class="labelChart">Documents: </li>
                                        <li class="labelChartB">
                                            @if (chart.lstDocuments == null || chart.lstDocuments.Count == 0)
                                            {
                                                <text>--No Documents--</text>
                                            }
                                            else
