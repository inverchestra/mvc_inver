@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.Cases

<script type="text/javascript">
    $(function () {
        $('ul.accordion').accordion({ collapsible: true });
    });
</script>

<link href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" rel="stylesheet" />

<div id="UpdateVisits" class="bluegrad" style="width: 100%">
    <h2>Visits</h2>
    <form id="frmUVisitId" enctype="multipart/form-data">
        <input type="hidden" name="Visits.CaseId" value="@Model.Visits.CaseId" />
        <input type="hidden" name="Visits.visitId" value="@Model.Visits.visitId" />
        <ul style="display: inline-block; padding: 13px 13px">
            <li>
                <div class="tldiv" style="width: 35%">
                    <h6>VisitDate:</h6>
                </div>
                <div class="trdiv" style="width: 55%">
                    <input type="text" name="Visits.CurrentVisitDate" id="cvisitDate2" value="@Model.Visits.CurrentVisitDate" title="Enter the visit date" class="create-logtxts" />
                </div>
            </li>
            <li>
                <div class="tldiv" style="width: 35%">
                    <h6>Next Visit:</h6>
                </div>
                <div class="trdiv" style="width: 55%">
                    <input type="text" name="Visits.NextVisitDate" value="@Model.Visits.NextVisitDate" class="create-logtxts" title="Enter the Hospital name " id="EditPProvider" />
                </div>
            </li>
            <li>
                <div class="tldiv" style="width: 35%">
                    <h6>Reason:</h6>
                </div>
                <div class="trdiv" style="width: 55%">
                    <input type="text" name="Visits.Reason" value="@Model.Visits.Reason" class="create-logtxts" title="Enter the Reason " />
                </div>
            </li>
            <li>
                <div class="tldiv" style="width: 35%">
                    <h6>Diet Prescription:</h6>
                </div>
                <div class="trdiv" style="width: 55%">
                    <input type="text" name="Visits.DietPresc" value="@Model.Visits.DietPresc" class="create-logtxts" title="Enter the Reason " />
                </div>
            </li>
            <li>
                <div class="tldiv" style="width: 35%">
                    <h6>Doctor's Log:</h6>
                </div>
                <div class="trdiv" style="width: 55%">
                    <input type="text" name="Visits.DoctorsLog" value="@Model.Visits.DoctorsLog" class="create-logtxts" title="Enter the Reason " />
                </div>
            </li>
            <li>
                <div class="tldiv" style="width: 35%">
                    <h6>Precautions:</h6>
                </div>
                <div class="trdiv" style="width: 55%">
                    <input type="text" name="Visits.Precautions" value="@Model.Visits.Precautions" class="create-logtxts" title="Enter the Reason" />
                </div>
            </li>
            <li>
                <ul class="accordion">
                    <li><a href="#nr">
                        <h5>Nurse Readings:</h5>
                    </a>
                        <ul id="nr">
                            <li>
                                <div class="tldiv" style="width: 35%">
                                    <h6>Height:</h6>
                                </div>
                                <div class="trdiv" style="width: 55%">
                                    <input type="text" name="Visits.NurseReadingsInfo[0].Height" value="@Model.Visits.NurseReadingsInfo.Height" class="create-logtxts" title="Enter the Reason " />
                                </div>
                            </li>
                            <li>
                                <div class="tldiv" style="width: 35%">
                                    <h6>Weight:</h6>
                                </div>
                                <div class="trdiv" style="width: 55%">
                                    <input type="text" name="Visits.NurseReadingsInfo[0].Weight" value="@Model.Visits.NurseReadingsInfo.Weight" class="create-logtxts" title="Enter the Reason " />
                                </div>
                            </li>

                            <li>
                                <div class="tldiv" style="width: 35%">
                                    <h6>Temperature:</h6>
                                </div>
                                <div class="trdiv" style="width: 55%">
                                    <input type="text" name="Visits.NurseReadingsInfo[0].Temparature" value="@Model.Visits.NurseReadingsInfo.Temparature" class="create-logtxts" title="Enter the Reason " />
                                </div>
                            </li>

                            <li>
                                <div class="tldiv" style="width: 35%">
                                    <h6>Blood Pressure:</h6>
                                </div>
                                <div class="trdiv" style="width: 55%">
                                    <input type="text" name="Visits.NurseReadingsInfo[0].Sistole" value="@Model.Visits.NurseReadingsInfo.Sistole" class="create-logtxts" title="Enter the Reason "  />
                                </div>
                            </li>
                        </ul>
                    </li>
                </ul>
            </li>
            <li>
                <input type="hidden" name="Visits.CaseId" value="@Model.Visits.CaseId" id="cid2" />
                <input type="hidden" name="Visits.visitId"  value="@Model.Visits.visitId" id="pid2" />
                <div style="width:100%" class="inline">
                    <input type="button" name="btnVisitUpdate" value="Update"  style="float:right; margin-right:10px;" class="greengrad" />
                </div>
            </li>
        </ul>
    </form>
</div>
