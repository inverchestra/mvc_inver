                $('#nav li a').removeClass('active');
                $(this).addClass('active');
            }
        });
        $("#dateOfServiceprocedurenotify1").hide();
        $("input[type='radio']").change(function () {

            if ($(this).val() == "true") {
                $("#dateOfServiceprocedurenotify1").show();

            }

            else {
                $("#dateOfServiceprocedurenotify1").hide();
            }

        }); 

