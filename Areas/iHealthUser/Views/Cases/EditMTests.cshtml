                            });

                            $('input[name="btnMTUpdate"]').click(function () {
                                if ($("#EditMTName").val() == "") {
                                    alert("Please enter medical test name");
                                    $('input[tabindex=1]').focus();
                                    //  return false;
                                }
                                else if ($("#dateOfMtest2").val() == "") {
                                    alert("Please enter medical test date");
                                    $('input[tabindex=2]').focus();
                                }
                                else {
