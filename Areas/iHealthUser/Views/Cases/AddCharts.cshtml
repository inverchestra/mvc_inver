@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.Cases
<link href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" rel="stylesheet" />
<div id="charts" class="bluegrad" style="width: 400px; height: 350px">
    <div style="border-bottom: 1px solid rgb(218,221,226);">
        <h3>
            Charts</h3>
    </div>
    <form id="frmct" enctype="multipart/form-data">
    <ul style="display: inline-block; padding: 13px 13px">
        <li>
            <input type="hidden" name="CaseId" id="CaseId" value="@Model.CaseId" />
        </li>
        <li>
            <div class="tldiv" style="width: 35%">
                <h6>
                    Chart name:</h6>
            </div>
            <div class="trdiv" style="width: 55%">
                <input type="text" name="Charts.ChartName" class="create-logtxts" title="Name of the chart"
                    required />
            </div>
        </li>
        <li>
            <div class="tldiv" style="width: 35%">
                <h6>
                    Chart type:</h6>
            </div>
            <div class="trdiv" style="width: 55%">
                <input type="text" name="Charts.ChartType" class="create-logtxts" title="Type of the chart"
                    required />
            </div>
        </li>
        <li>
            <div class="tldiv" style="width: 35%">
                <h6>
                    Chart date :</h6>
            </div>
            <div class="trdiv" style="width: 55%">
                <input type="text" id="dateOfChart3" name="Charts.ChartDate" placeholder="MM/DD/YYYY"
                    title="Chart date" pattern="(0[1-9]|1[012])[- /.](0[1-9]|[12][0-9]|3[01])[- /.]\d\d\d\d"
                    class="create-logtxts" required />
            </div>
        </li>
        <li>
            <div class="tldiv" style="width: 35%">
                <h6>
                    Chart details:</h6>
            </div>
            <div class="trdiv" style="width: 55%">
                <input type="text" name="Charts.ChartDetails" class="create-logtxts" title="Details of the chart"
                    required />
            </div>
        </li>
        <li>
            <div class="tldiv" style="width: 35%">
                <h6>
                    Comments:</h6>
            </div>
            <div class="trdiv" style="width: 55%">
                <input type="text" name="Charts.Comments" class="create-logtxts" title="Comments for the chart"
                    required />
            </div>
        </li>
        <li>
            <div class="tldiv" style="width: 35%">
                <h6>
                    Chart documents:</h6>
            </div>
            <div class="trdiv" style="width: 55%">
                <input type="file" name="MTfiles" title="Select the document" multiple="multiple" />
            </div>
        </li>
        <li>
            <div style="width: 100%" class="inline">
                <input type="button" name="cancel" value="Cancel" style="float: right" class="graygrad" />
                <input type="submit" id="btnCTSave" name="save" value="Save" style="float: right;
                    margin-right: 10px;" class="greengrad" />
            </div>
        </li>
    </ul>
    </form>
</div>
<script>
    (function (i, s, o, g, r, a, m) {
        i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
            (i[r].q = i[r].q || []).push(arguments)
        }, i[r].l = 1 * new Date(); a = s.createElement(o),
  m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

