@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.Cases
<link href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" rel="stylesheet" />
<script type="text/javascript">
    $(function () {
        $('#dateOfChart11').datepicker({
            changeYear: true,
            showAnim: 'slideDown',
            changeMonth: true
        });
    });

</script>
<div id="chartsdiv" class="bluegrad" style="width: 490px;">
    <div style="border-bottom: 1px solid rgb(218,221,226);">
        <h3>
            Charts</h3>
    </div>
    <form id="frmcts" enctype="multipart/form-data" class="crtproc">
    <ul style="display: inline-block; padding: 13px 13px">
        <li class="labelnormal">Chart name:<span style="font-weight: 600; padding: 15px 0px 0px 2px;
            color: rgb(251, 2, 2); font-size: medium;">*</span></li>
        <li>
            <input type="text" name="Charts.ChartName" id="chrtnameid" tabindex='1' class="create-logtxts"
                title="Name of the Chart" />
            <input type="hidden" name="CaseId" id="CaseId" value="@Model.CaseId" />
        </li>
        <li class="labelnormal">Chart type:<span style="font-weight: 600; padding: 15px 0px 0px 2px;
            color: rgb(251, 2, 2); font-size: medium;">*</span></li>
        <li>
            <input type="text" name="Charts.ChartType" id="chrttypeid" tabindex='2' class="create-logtxts"
                title="Type of the Chart" required />
        </li>
        <li class="labelnormal">Chart date :<span style="font-weight: 600; padding: 15px 0px 0px 2px;
            color: rgb(251, 2, 2); font-size: medium;">*</span></li>
        <li>
            <input type="text" id="dateOfChart11" name="Charts.ChartDate" placeholder="MM/DD/YYYY"
                tabindex='3' title="Chart Date" class="create-logtxts" required />
        </li>
        <li class="labelnormal">Chart details:</li>
        <li>
            <input type="text" name="Charts.ChartDetails" class="create-logtxts" title="Details of the chart"
                tabindex="4"  />
        </li>
        <li class="labelnormal">Comments:</li>
        <li>
            <input type="text" name="Charts.Comments" class="create-logtxts" title="Comments for the Chart"
                 tabindex='5' />
        </li>
        <li class="labelnormal">Chart documents:</li>
        <li>
            <input type="file" name="MTfiles" title="Select the Document" multiple="multiple"
                tabindex='6' />
        </li>
        <li style="width: 91%" class="inline">
            <div>
                <input type="button" name="cancel" tabindex="7" value="Cancel" style="float: right" class="graygrad"
                    onclick="$.modal.close()" />
                <input type="submit" id="btnCTSave" tabindex="8" name="save" value="Save" style="float: right;
                    margin-right: 10px;" class="greengrad" />
            </div>
        </li>
    </ul>
    </form>
</div>
<script>
