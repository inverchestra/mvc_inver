@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.Cases
@{
    int i = 1;
}


<h3>Schedule Info</h3>

<div id="scheduleInfo">
        <div style="display: inline-block; width: 100%; padding: 10px; color: rgb(89,89,89); font-size: 12px; font-weight: bold">

            <div id="hspdoc-div">
                @if (Model.MSchedule.Count  == 0)
                {
                    <br />
                <h6><span>You have no Schedule info for this Case.</span></h6>
                }
                else
                {
                    <ul>
                        @foreach (var mshdl in Model.MSchedule)
                        {
                        
                                try
                                {
                            <li>
                                <div class="tldiv">
                                    <h6>MedicineName:</h6>
                                </div>
                                <div class="trdiv">
                                    <input type="text" name="MSchedule[@i].MedicineName" value="@mshdl.MedicineName" class="create-logtxts" />
                                </div>
                            </li>
                        @*<li>
                            <div class="tldiv">
                                <h6>DoctorName</h6>
                            </div>
                            <div class="trdiv">
                                <textarea name="MSchedule[@i].DoctorName" rows="3" cols="20" class="create-logtxts" style="height: 40px">@hospital.DoctorName</textarea>
                            </div>
                        </li>*@

                        }
                                finally { i++; }
                        }
                    </ul>
                }
            </div>




        </div>
    </div>
