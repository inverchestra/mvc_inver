@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.Cases
@{
    int i = 0;
    int l = 0;
}
<script type="text/javascript">

    //$('#nav > li > a, .nav_UV> li > a').click(function (e) {
    //    e.preventDefault();
    //    if ($(this).attr('class') != 'active') {
    //        $('#nav li ul,.nav_UV li ul').slideUp();
