@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.Cases
<link href="http://code.jquery.com/ui/1.10.0/themes/base/jquery-ui.css" rel="stylesheet" />
<script src="~/CustomScripts/createvisit.js" type="text/javascript"></script>
<div id="visit" class="bluegrad" style="width: 580px; min-height: 420px; height: auto;
    max-height: 434px; overflow: auto; overflow-x: hidden">
    <div style="border-bottom: 1px solid rgb(218,221,226);">
        <h3>
            Create Visit</h3>
    </div>
    <form id="frmvst" enctype="multipart/form-data" class="visit" >
    <input type="hidden" name="CaseId" value="@Model.CaseId" />
    <ul style="display: inline-block; margin: 10px 0 0;">
        <li class="labelnormal">Visit date: <span style="font-weight: 600; padding: 15px 0px 0px 2px;
            color: rgb(251, 2, 2); font-size: medium;">*</span></li>
        <li>
            <input type="text" name="Visits.CurrentVisitDate" id="datevisit1" placeholder="MM/DD/YYYY"
                tabindex='1' title="Test service date" class="create-logtxts" />
        </li>
        <li class="labelnormal">Reason: <span style="font-weight: 600; padding: 15px 0px 0px 2px;
            color: rgb(251, 2, 2); font-size: medium;">*</span></li>
        <li>
            <input type="text" id="reason" name="Visits.Reason" tabindex='2' title="Reason to visit"
                class="create-logtxts" />
        </li>
        <li class="labelnormal">Doctors comment: </li>
        <li>
            <textarea id="DoctorsLog" name="Visits.DoctorsLog" cols="20" rows="20" class="create-logtxts" tabindex='3'
                title="Describe doctors comment" style="height: 30px"></textarea>
        </li>
        <li class="labelnormal">Diet prescription: </li>
        <li>
            <textarea id="DietPrescription" cols="20" rows="20" name="Visits.DietPresc" class="create-logtxts" tabindex='4'
                title="Dietprescription" style="height: 30px;"></textarea>
        </li>
        <li class="labelnormal">Precautions: </li>
        <li>
            <textarea name="Visits.Precautions" class="create-logtxts" cols="20" rows="20" title="Precautions" tabindex='5'
                style="height: 30px"></textarea>
        </li>
        <li class="labelnormal">Nurse readings:
            <img id="imgadd1" src="~/Images/add.png" title="Add NurseReadings" alt="add"  tabindex='6'/>
        </li>
        <li>
            <div id="NRInfos" style="width: 200px">
            </div>
        </li>
        <li class="labelnormal">Doctor Information:
            <img id="imgad2" src="~/Images/add.png" title="Add DoctorInfo" alt="add"  tabindex='7'/>
        </li>
        <li>
            <div id="DoctorInfos" style="width: 200px">
            </div>
        </li>
        <li class="labelnormal">Next visit date: </li>
        <li>
            <input type="text" name="Visits.NextVisitDate" placeholder="MM/DD/YYYY" id="nextdatevisit1" tabindex='8'
                title="next visit date" pattern="(0[1-9]|1[012])[- /.](0[1-9]|[12][0-9]|3[01])[- /.]\d\d\d\d"
                class="create-logtxts" />
        </li>
        <li class="labelnormal">Do you want to be notified?: </li>
        <li>
            <input type="radio" name="Visits.IsNotify" value="true" required />&nbsp;Yes
            <input type="radio" name="Visits.IsNotify" value="false" checked="checked" />&nbsp;No
        </li>
        <li style="width: 100%" class="inline">
            <div style="width: 100%" class="inline">
                <input type="button" name="cancel" value="Cancel" style="float: right;" class="graygrad" tabindex='9'
                    onclick="$.modal.close()" />
                <input type="submit" id="btnVstSave" value="Save" style="float: right; margin-right: 10px;" tabindex='10'
                    class="greengrad" />
            </div>
        </li>
    </ul>
    </form>
    <div id="DoctorInfo" class="slide">
        <ul style="display: inline-block; padding: 13px 13px">
            <li>Doctor name: </li>
            <li>
                <input type="text" id="doctorname" name="DoctorName" title="Enter the doctor name"
                    class="create-logtxts" required />
            </li>
            <li>Doctor phone number: </li>
            <li>
                <input type="text" id="doctorphno" name="DoctorPhno" title="Enter the phno" class="create-logtxts" />
            </li>
            <li>Doctor email: </li>
            <li>
                <input type="text" id="demail" name="DoctorEmail" title="Enter doctor email" placeholder="me@example.com"
                    class="create-logtxts" />
            </li>
        </ul>
        <div class="inline accord_btn">
            <input type="button" name="cancel1" value="Cancel" class="graygrad" style="float: right;" />
            <input type="button" id="DoctorInfoSave" value="Save" style="float: right;" class="greengrad" />
        </div>
    </div>
    <div id="NRInfo" class="slide">
        <ul style="display: inline-block; padding: 13px 13px">
            <li>Height: </li>
            <li>
                <input type="text" id="Heightfeet" title="Enter the height" name="Height" class="create-logtxts"
                    placeholder="Height" required style="width: 100px;" />Cms </li>
            <li>Weight: </li>
            <li>
                <input type="text" id="Weight" name="Weight" title="Enter the weight" class="create-logtxts"
                    placeholder="Weight" style="width: 100px;" />Kgs </li>
            <li>Temperature: </li>
            <li>
                <input type="text" id="Temperature" name="Temparature" title="Enter the temperature value"
                    class="create-logtxts" placeholder="'F" style="width: 100px;">
            </li>
            <li>Blood pressure: </li>
            <li>
                <input type="text" id="Sistole" name="Sistole" title="Enter the systole value" placeholder="Systole"
                    class="create-logtxts" style="width: 100px;" />/
                <input type="text" id="Diastole" name="Diastole" title="Enter the diastole value"
                    placeholder="Diastole" class="create-logtxts" style="width: 100px;" />
            </li>
            <li>Respiratory rate: </li>
            <li>
                <input type="text" id="Respiratoryrate" name="Respiratoryrate" title="Enter the respiratory value "
                    placeholder="Respirate" class="create-logtxts" style="width: 100px;" />Per min
            </li>
            <li>Pulse rate: </li>
            <li>
                <input type="text" id="PulseRate" name="PulseRate" title="Enter the pulserate value"
                    class="create-logtxts" placeholder="pulserate" style="width: 100px;" />Per min
            </li>
        </ul>
        <div style="width: 100%" class="inline">
            <input type="button" name="cancel1" value="Cancel" style="float: right" class="graygrad" />
            <input type="button" id="NRInfoSave" value="Save" style="float: right; margin-right: 10px;"
                class="greengrad" />
        </div>
    </div>
</div>
