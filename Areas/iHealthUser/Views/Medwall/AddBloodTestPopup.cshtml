@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
<script type="text/javascript">
    $(function () {
        $("#submitBT").click(function () {

            var a = $('#subscription_order_form123').serialize();
            $.ajax({
                type: 'POST',
                url: '/iHealthUser/Medwall/AddBloodTest',
                data: a,
                success: function (data) {
                    $.modal.close();
                    SetData(data);
                }
            });

        });
        function SetData(data) {
            $("#tabs-2").html(data);
        }
    });
</script>
<div>
    <form id="subscription_order_form123" class="" method="post" enctype="multipart/form-data">
    <div style="width: 400px">
        <div style="border-bottom: 1px solid #d5d5d5">
            <span style="font-size: 15px; font-family: Segoe UI; width: 300px">@ViewBag.BloodTestName</span>
            &nbsp;&nbsp
            <input type="hidden" value="@ViewBag.BloodTestName" name="BTName" />
            @Html.TextBoxFor(model => model.BT.BloodTestValue, new { @id = "heBloodTestNameight", @placeholder = @ViewBag.BloodTestValue, title = @ViewBag.BloodTestValue })
        </div>
        <div style="margin-top: 10px">
            <input type="button" name="btnAddBTCancel" class="graygrad" value="Cancel" onclick="$.modal.close();" />
            <input type="submit" id="submitBT" class="greengrad" value="submit" />
        </div>
    </div>
    </form>
</div>
