@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals


<div style="width: 550px; overflow:auto;">
    <table id="pra">
        <tbody>
            <tr>
                <td style="width:150px; color:#0088cc; font-size:13px">
                    Date
                </td>
                 @foreach (var item in Model.lstSkinTest)
                 {
                <td style="font-weight:600; color:Gray; width:80px;">
                        @item.dateST
                </td>
                 }
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                   Color of nipples
                </td>
                
                    @foreach (var item in Model.lstSkinTest)
                    {
                       if(item.ColourOfNipples==true)
                        { 
                            <td>
                            Yes
                        </td>
                        }
                       else
                       {
                             <td>
                            No
                        </td>
                       }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Pigmentation spots
                </td>
                
                    @foreach (var item in Model.lstSkinTest)
                    {
                       if(item.PigmentationSpotsOnSkin==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                       else
                       {
                             <td>
                            No
                        </td>
                       }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                     Complexion of skin
                </td>
                
                    @foreach (var item in Model.lstSkinTest)
                    {
                       if(item.ComplexionOfSkin==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                       else
                       {
                             <td>
                            No
                        </td>
                       }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Stretch marks
                </td>
                
                    @foreach (var item in Model.lstSkinTest)
                        
                    {
                        if(item.StretchMarks==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                        else
                        {
                             <td>
                            No
                        </td>
                        }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Acne
                </td>
                
                    @foreach (var item in Model.lstSkinTest)
                    {
                        if(item.Acne==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                        else
                        {
                             <td>
                            No
                        </td>
                        }
                    }
                
            </tr>
        </tbody>
    </table>
</div>

