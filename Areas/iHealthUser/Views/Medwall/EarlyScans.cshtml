@model IEnumerable<InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.EarlyScan>
<script type="text/javascript">
    $(function () {
        $('input[name="earlyscan"]').click(function () {
            var esid = $(this).siblings(':hidden').val();
            //alert(esid);
            $.ajax({
                url: '/iHealthUser/Medwall/EarlyScanView',
                type: 'GET',
                data: { esid: esid },
                success: function (result) {
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        position: ["2%", "30%"],
                        closeHTML: "",
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }

                    });
                }
            });
        });
    });
</script>
<div id="proctable">
    
    @if (Model.Count() > 0)
    {     
      <ul class="li_color" style="margin:0px">
               @foreach (var value in Model)
               {
                   <li>
                      @value.CreatedOn
                            <div style="float:right; display: inline-block">
                                <input type="hidden" name="esname" value="@value.EarlyScanId" />
                                <input type="button" id="earlyscan" name="earlyscan" class="viewbutton" value="View" />
                            </div>
                       </li>                          
               }
          </ul>  
    }
    else
    {
        <text>No scans</text>
    }
            
</div>


<div id="tab" style="display: none">
</div>
