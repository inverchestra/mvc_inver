@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
@{
    ViewBag.Title = "";
}
<script type="text/javascript">
    $(function () {
        $("#btnUpdatestest").click(function () {
            $.ajax({
                url: '/iHealthUser/Medwall/UrineTestEdit',
                type: 'GET',
                success: function (result) {
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                            $("input[name = 'btnCancel']").click(function () {
                                $.modal.close();
                            });
                        }
                    });
                }
            });
        });

        $("#btnUrineTestTrend").click(function () {
            $.ajax({
                url: '/iHealthUser/Medwall/UrineTestTrend',
                type: 'GET',
                success: function (result) {

                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }
                    });
                }
            });
        });
    });


</script>
<div style="border-bottom: 1px solid rgb(218,221,226); width: 100%;">
    @if (Model.UT.CreatedOn != null) {
        <lable>Recent updation on:</lable> 
        <label style="color: Red">
            (@Html.DisplayTextFor(model => model.UT.CreatedOn) )</label>
    }
    <div class="vitals">
        <ul>
            @if (Model.UT != null) {  
                <li style="border-bottom: 1px dashed #badbfa">
                    <div>
                        <div style="width: 40%; display: inline-block">
                            Glucose tolerant test :</div>
                        <div style="width: 40%; display: inline-block">
                            @{ var GlucoseTolerantTest = Model.UT.GlucoseTolerantTest ? "Yes" : "No";}
                            <label title="GlucoseTolerantTest">@GlucoseTolerantTest</label>
                        </div>
                    </div>
                </li>
                
                
                <li style="border-bottom: 1px dashed #badbfa">
                    <div>
                        <div style="width: 40%; display: inline-block">
                            Urinary tract infection :</div>
                        <div style="width: 40%; display: inline-block">
                            @{ var UrinaryTtractInFection = Model.UT.UrinaryTtractInFection ? "Yes" : "No";}
                            <label title="UrinaryTtractInFection">@UrinaryTtractInFection</label>
                        </div>
                    </div>
                </li>
                
                <li style="border-bottom: 1px dashed #badbfa">
                    <div>
                        <div style="width: 40%; display: inline-block">
                            Protein :</div>
                        <div style="width: 40%; display: inline-block">
                            @{ var Protein = Model.UT.Protein ? "Yes" : "No";}
                            <label title="Protein">@Protein</label>
                        </div>
                    </div>
                </li>
                
                <li style="border-bottom: 1px dashed #badbfa">
                    <div>
                        <div style="width: 40%; display: inline-block">
                            Blood cells :</div>
                        <div style="width: 40%; display: inline-block">
                            @{ var BloodCells = Model.UT.BloodCells ? "Yes" : "No";}
                            <label title="BloodCells">@BloodCells
                            </label>
                        </div>
                    </div>
                </li>
                
                
                <li style="border-bottom: 1px dashed #badbfa">
                    <div>
                        <div style="width: 40%; display: inline-block">
                            Ketones :</div>
                        <div style="width: 40%; display: inline-block">
                            @{ var Ketones = Model.UT.Ketones ? "Yes" : "No";}
                            <label title="Ketones">@Ketones</label>
                        </div>
                    </div>
                </li>
                
              
         
            } else {
                <li>No data is available.</li>
            
                <li style="width: 100%" class="inline">
                    <div style="width: 100%; float: right; margin: 0 7px 0 0;" class="inline">
                    </div>
                </li>
            }
            <li style="width: 100%" class="inline">
                <input id="btnUpdatestest" type="button" class="greengrad" value="Newdata" style="margin-right: 10px" />
            </li>
           @* <li>
                <input type="button" value="Trend" id="btnUrineTestTrend" class="graygrad" />
            </li>*@
        </ul>
    </div>
</div>
