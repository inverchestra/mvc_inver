<link href="~/Content/resolution.css" rel="stylesheet" />
<script type="text/javascript">
    $(function () {


        $('div#BloodTests').load('/iHealthUser/Medwall/GetBloodTests');
        $('div#UrineTests').load('/iHealthUser/Medwall/GetUrineTests');
        $('div#SeerumTests').load('/iHealthUser/Medwall/GetSeerumTests');

        $('#btnAddBloodTest').click(function () {
            $.ajax({
                url: '/iHealthUser/Medwall/CBPView',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        position: ["2%", "30%"],
                        closeHTML: "",

                        onShow: function (e) {

                            e.container.css({ 'height': 'auto', 'width': 'auto', 'top': '0px !important' });

                        }

                    });
                }
            });
        });
        $('#btnAddUrineTest').click(function () {
            $.ajax({
                url: '/iHealthUser/Medwall/AddComplUrine',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
