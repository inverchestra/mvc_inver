                        closeHTML: "",

                        onShow: function (e) {

                            e.container.css({ 'height': 'auto', 'width': 'auto', 'top': '0px !important' });

                        }
                    });
                }
            });
        });


        $('#btnAddSeerumTest').click(function () {
            $.ajax({
                url: '/iHealthUser/Medwall/SerumTest',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        //position: ["2%", "30%"],
                        closeHTML: "",

                        onShow: function (e) {

                            e.container.css({ 'height': 'auto', 'width': 'auto', 'top': '0px !important' });

                        }
                    });
                }
            });
        });


        //        $('#btnget').click(function () {
        //            $.ajax({
        //                url: '/iHealthUser/Medwall/GetUrieneTests',
        //                type: 'GET',

        //                success: function (result) {
        //                    $.modal.close();
        //                    $('#ajaxtab').html(result).modal({
        //                        escClose: true,
        //                        closeHTML: "",
        //                        onShow: function (e) {
        //                            e.container.css({ 'height': 'auto', 'width': 'auto' });

        //                        }

        //                    });

        //                }
        //            });
