@using InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository
@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.UltraScans
@{
    ViewBag.Title = "SelectScanType";
    UltraScans obj = new UltraScans();
    List<SelectListItem> Stypes = obj.ScanTypes();
}
<script type="text/javascript">
    $(document).ready(function () {
        $("#btnContinue").click(function () {





            var drop = $('#Scandrop').val();


            if ($('#Scandrop').val() == "") {
                alert("Please select one item");
            }
            else if ($('#Scandrop').val() == "EarlyStage") {
                //                    alert("Hi Family");
                document.getElementsByName('SType').value = $('#Scandrop').val();

                var val1 = document.getElementsByName('SType').value;

                $.modal.close();
                $('#ajaxtab').empty();
                $.ajax({
                    url: '/iHealthUser/Medwall/EarlyScan',
                    type: 'GET',
                    data: { value1: val1 },
                    success: function (result) {
                        $.modal.close();
                        $('#ajaxtab').html(result).modal({
                            escClose: true,
                            closeHTML: ""
                        });
                    }
                });
            }
            else if ($('#Scandrop').val() == "NTScan") {
                //                    alert("Hi Family");
                document.getElementsByName('SType').value = $('#Scandrop').val();

                var val1 = document.getElementsByName('SType').value;

                $.modal.close();
                $('#ajaxtab').empty();
                $.ajax({
                    url: '/iHealthUser/Medwall/NTScan',
                    type: 'GET',
                    data: { value1: val1 },
                    success: function (result) {
                        $.modal.close();
                        $('#ajaxtab').html(result).modal({
                            escClose: true,
                            closeHTML: ""
                        });
                    }
                });
            }
            else if ($('#Scandrop').val() == "AnamolyScan") {
                //                    alert("Hi Family");
                document.getElementsByName('SType').value = $('#Scandrop').val();

                var val1 = document.getElementsByName('SType').value;

                $.modal.close();
                $('#ajaxtab').empty();
                $.ajax({
                    url: '/iHealthUser/Medwall/AnamolyScan',
                    type: 'GET',
                    data: { value1: val1 },
                    success: function (result) {
                        $.modal.close();
                        $('#ajaxtab').html(result).modal({
                            escClose: true,
                            closeHTML: ""
                        });
                    }
                });
            }
            else if ($('#Scandrop').val() == "Growth") {
                //                    alert("Hi Family");
                document.getElementsByName('SType').value = $('#Scandrop').val();

                var val1 = document.getElementsByName('SType').value;

                $.modal.close();
                $('#ajaxtab').empty();
                $.ajax({
                    url: '/iHealthUser/Medwall/GrowthScan',
                    type: 'GET',
                    data: { value1: val1 },
                    success: function (result) {
                        $.modal.close();
                        $('#ajaxtab').html(result).modal({
                            escClose: true,
                            closeHTML: ""
                        });
                    }
                });
            }



        });
    });
</script>
      
       
<h2>SelectScanType</h2>

<ul>
  <li class="labelnormal" style="margin-left: 3%; width: 90%;">
                <div  style="width: 100%; padding: 10px; color: rgb(89,89,89);
                    font-size: 12px; font-weight: bold">
                    <div style="float: left; width: 40%;">
                        Select ScanType:
                    </div>
                    <div style="display: inline-block; width: 60%;">
                        @Html.DropDownListFor(model => model.ScanType, Stypes, new { @id = "Scandrop", @class = "Reg_Details_select" })
                       

                    </div>
                </div>
            </li>
              <li class="labelnormal" style="width: 97%; margin: 10px 0 0 0;">
             <input type="button" value="Cancel" class="graygrad_FP" onclick="$.modal.close();" />
            <input type="button" value="Continue" id="btnContinue" class="greengrad_FP" style="margin-right:10px"/>
            </li>
</ul>


