@model IEnumerable<InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.NTScan>
<script type="text/javascript">
    $(function () {
        $('input[name="ntscan"]').click(function () {
            var ntid = $(this).siblings(':hidden').val();

            $.ajax({
                url: '/iHealthUser/Medwall/NTScanview',
                type: 'GET',
                data: { ntid: ntid },
                success: function (result) {
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        position: ["2%", "30%"],
                        closeHTML: "",
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }

                    });
                }
            });
        });
        
    });
</script>
<div id="proctable">
    <div>
       
    </div>
    @if (Model.Count() > 0)
    {     
        <ul class="li_color" style="margin:0px">
                @foreach (var item in Model)
                {
                    <li>
                        @item.CreatedOn
                        <div style="float:right; display: inline-block">
                            <input type="button" id="ntscan" name="ntscan" class="viewbutton" value="View" />
                            <input type="hidden" id="ntid" value="@item.NtScanId" />
                        </div>
                        </li>
                }
            </ul>
    }
    else
    {
        <text>No scans</text>
    }
</div>


<div id="tab" style="display: none">
</div>
