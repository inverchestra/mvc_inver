@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
<h2>
    S T D's</h2>
@{
    ViewBag.Title = "";
}
<script type="text/javascript">
    $('#btnstdsupdate').click(function () {

        var a = $('#subscription_order_formstds').serialize();
        $.ajax({
            type: 'post',
            url: '/iHealthUser/Medwall/STDs',
            data: a,
            success: function (data) {
                $.modal.close();
                SetData(data);
            }
        });
    });
    function SetData(data) {
        $("#vitalsDiv").html(data); // HTML DOM replace
    }
</script>
