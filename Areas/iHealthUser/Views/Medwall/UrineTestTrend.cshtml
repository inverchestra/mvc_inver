@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
<div style="width: 550px; overflow:auto;">
    <table id="pra">
        <tbody>
            <tr>
                <td style="width:150px; color:#0088cc; font-size:13px">
                    Date
                </td>
                 @foreach (var item in Model.lstUrineTest)
                 {
                <td style="font-weight:600; color:Gray; width:80px">
                        @item.dateUT
                </td>
                 }
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Glucose tolerant test
                </td>
                
                    @foreach (var item in Model.lstUrineTest)
                    {
                        if(item.GlucoseTolerantTest==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                        else
                        {
                             <td>
                            No
                        </td>
                        }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Urinary tract infection
                </td>
                
                    @foreach (var item in Model.lstUrineTest)
                    {
                        if(item.UrinaryTtractInFection==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                        else
                        {
                             <td>
                            No
                        </td>
                        }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Protein
                </td>
                
                    @foreach (var item in Model.lstUrineTest)
                    {
                        if(item.Protein==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                        else
                        {
                             <td>
                            No
                        </td>
                        }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Blood cells
                </td>
                
                    @foreach (var item in Model.lstUrineTest)
                        
                    {
                        if(item.BloodCells==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                        else
                        {
                             <td>
                            No
                        </td>
                        }
                    }
                
            </tr>
            <tr>
                <td style="color:#0088cc; font-size:15px">
                    Ketones
                </td>
                
                    @foreach (var item in Model.lstUrineTest)
                    {
                        if(item.Ketones==true)
                        {
                            <td>
                            Yes
                        </td>
                        }
                        else
                        {
                             <td>
                            No
                        </td>
                        }
                    }
                
            </tr>
        </tbody>
    </table>
</div>
