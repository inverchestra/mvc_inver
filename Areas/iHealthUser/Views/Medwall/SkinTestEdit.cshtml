                            style="margin: 0 0 0 5px">No</span>
                    </label>
                }
                else if (Model.ST.PigmentationSpotsOnSkin == true)
                {
                    <label>
                        <input type="radio" name="ST.PigmentationSpotsOnSkin" value="true" checked="checked" /><span
                            style="margin: 0 0 0 5px">Yes</span>
                    </label>
                    <label>
                        <input type="radio" name="ST.PigmentationSpotsOnSkin" value="false" /><span style="margin: 0 0 0 5px">No</span>
                    </label>
                }
            </li>
            <li style="width: 100%; display: inline-block; background-color: #f7f7f7">
                <label style="font-size: medium">
                    Complexion of skin :</label>
                @if (Model.ST.ComplexionOfSkin == false)
                {
                    <label style="margin-left: 55px">
                        <input type="radio" name="ST.ComplexionOfSkin" value="true" /><span style="margin: 0 0 0 5px">Yes</span>
                    </label>
                    <label>
                        <input type="radio" name="ST.ComplexionOfSkin" value="false" checked="checked" /><span
                            style="margin: 0 0 0 5px">No</span>
                    </label>
                }
                else if (Model.ST.ComplexionOfSkin == true)
