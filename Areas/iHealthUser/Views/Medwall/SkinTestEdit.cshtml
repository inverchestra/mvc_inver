@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
<h2>
    Update your Skin test</h2>
@{
    ViewBag.Title = "";
}
<script type="text/javascript">
    $('#BtnSkinTestupdate').click(function () {

        var a = $('#subscription_order_formstds').serialize();
        $.ajax({
            type: 'post',
            url: '/iHealthUser/Medwall/SkinTest',
            data: a,
            success: function (data) {
                $.modal.close();
                SetData(data);
            }
        });
    });
    function SetData(data) {
        $("#vitalsDiv").html(data); // HTML DOM replace
    }
</script>
<div style="width: 350px">
    <form id="subscription_order_formstds" class="" method="post" enctype="multipart/form-data">
    <div class="PinfoViewInfo">
        <ul>
            <li style="width: 100%; display: inline-block; background-color: #f7f7f7">
                <label style="font-size: medium">
                    Color of nipples :</label>
                @if (Model.ST.ColourOfNipples == false)
                {
                    <label style="margin-left: 79px">
                        <input type="radio" name="ST.ColourOfNipples" value="true" /><span style="margin: 0 0 0 5px">Yes</span>
                    </label>
                    <label>
                        <input type="radio" name="ST.ColourOfNipples" value="false" checked="checked" /><span
                            style="margin: 0 0 0 5px">No</span>
                    </label>
                }
                else if (Model.ST.ColourOfNipples == true)
                {
                    <label style="margin-left: 79px">
                        <input type="radio" name="ST.ColourOfNipples" value="true" checked="checked" /><span
                            style="margin: 0 0 0 5px">Yes</span>
                    </label>
                    <label>
                        <input type="radio" name="ST.ColourOfNipples" value="false" /><span style="margin: 0 0 0 5px">No</span>
                    </label>
                }
            </li>
            <li style="width: 100%; display: inline-block; background-color: #f7f7f7">
                <label style="font-size: medium">
                    Pigmentation spots on skin :</label>
                @if (Model.ST.PigmentationSpotsOnSkin == false)
                {
                    <label>
                        <input type="radio" name="ST.PigmentationSpotsOnSkin" value="true" /><span style="margin: 0 0 0 5px">Yes</span>
