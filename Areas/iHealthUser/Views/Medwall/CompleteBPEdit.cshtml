@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
<script type="text/javascript">
    $(function () {
        $('#btnBloodInfoSubmit').click(function () {

            var a = $('#subscription_order_form_ust').serialize();

            $.ajax({

                url: '/iHealthUser/Medwall/CompleteBPEdit',
                type: 'POST',
                data: a,
                success: function (data) {
                    $.modal.close();
                    SetData(data);
                }
            });

        });
        function SetData(data) {
            $("#tabs-2").html(data);
        }
    });
</script>
<div style="width: 350px">
    <h2>
        Blood info</h2>
    <form id="subscription_order_form_ust" class="" method="post" enctype="multipart/form-data">
    <div class="PinfoViewInfo">
        <ul>
            <li class="PinfoViewInfo_lbl Faminfo_gap">H.I.V. :</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.HIV, new { title = "HIV", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">Hemoglobin count :</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.HemoglobinCount, new { title = "HemoglobinCount", @placeholder = "g/dL", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">Blood sugar :</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.BloodSugar, new { title = "BloodSugar", @placeholder = "mmol/L", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">Hepatitis B virus :</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.HepatitisBVirus, new { title = "HepatitisBVirus", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">Rubella(German measles) :</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.Rubella, new { title = "Rubella", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">Herpes :</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.Herpes, new { title = "Herpes", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">Platelet count</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.PlateletCount, new { title = "PlateletCount", @placeholder = "X 10^3/mm3", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">White blood cell count</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.WhiteBloodCellCount, new { title = "WhiteBloodCellCount", @placeholder = "X 10^3/mm3", @class = "logintxts" })
            </li>
            <li class="PinfoViewInfo_lbl Faminfo_gap">Red blood cell count</li>
            <li class="Faminfo_gap_value">
                @Html.TextBoxFor(model => model.CompleteBP.RedBloodCellCount, new { title = "RedBloodCellCount", @placeholder = "X 10^6/mm3", @class = "logintxts" })
            </li>
            <li style="width: 100%" class="inline">
                <input type="button" name="btnCancel" class="graygrad" value="Close" onclick="$.modal.close();" />
                <input id="btnBloodInfoSubmit" type="button" class="greengrad" value="Submit" style="margin-right: 10px" />
            </li>
        </ul>
    </div>
    </form>
</div>
