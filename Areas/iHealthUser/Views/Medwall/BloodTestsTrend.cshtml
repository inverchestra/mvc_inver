@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
<div style="width: 550px; overflow: auto;">
    <table id="pra">
        <tbody>
            <tr>
                <td style="width: 150px; color: #0088cc; font-size: 13px">
                    Date
                </td>
                @foreach (var item in Model.LstBT)
                {
                    <td style="font-weight: 600; color: Gray; width: 80px;">
                        @item.dateBloodTests
                    </td>
                }
            </tr>
            <tr>
                <td style="color: #0088cc; font-size: 15px">
                    @ViewBag.BTname
                </td>
                @foreach (var item in Model.LstBT)
                {
                    <td>
                        @item.BloodTestValue
                    </td>
                }
            </tr>
        </tbody>
    </table>
</div>
