@model InnDocs.iHealth.Web.UI.Models.UserInformation
<div id="AddUserDiv" class="bluegrad" style="width: 400px;">
    <form id="AddUser" action="@Url.Action("CreateUser", "AccountSettings")" method="post" style="width: 100%" >
    <div style="height: 40px;">
        <h4 class="PMoreInfo_btm">
            User Details</h4>
    </div>
    <div style="width: 100%">
        @Html.HiddenFor(model => Model.UserId)
        <ul>
            <li style="width: 100%" class="inline">
                <div class="tldiv inline">
                    @Html.HiddenFor(model => Model.UserId)
                    <h6>
                        First name</h6>
                </div>
                <div class="trdiv inline">
                    @Html.TextBoxFor(model => Model.FirstName, new { @class = "create-logtxts" })
                </div>
            </li>
            <li style="width: 100%" class="inline">
                <div class="tldiv inline">
                    <h6>
                        Last name</h6>
                </div>
                <div class="trdiv inline">
                    @Html.TextBoxFor(model => Model.LastName, new { @class = "create-logtxts" })
                </div>
            </li>
            <li style="width: 100%" class="inline">
                <div class="tldiv inline">
                    <h6>
                        Login name</h6>
                </div>
                <div class="trdiv inline">
                    @Html.TextBoxFor(model => Model.LoginName, new { @class = "create-logtxts" })
                </div>
            </li>
            <li style="width: 100%" class="inline">
                <div class="tldiv inline">
                    <h6>
                        Relationship</h6>
                </div>
                <div class="trdiv inline">
                    @Html.TextBoxFor(model => Model.Relationship, new { @class = "create-logtxts" })
                </div>
            </li>
            <li style="width: 100%" class="inline">
                <div style="float: right; padding-left: 10px; height: 40px;">
                    Gender: &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;
                    <input type="radio" name="Gender" value="Male" checked="true" />&nbsp; Male &nbsp;&nbsp;
                    <input type="radio" name="Gender" value="FeMale" />&nbsp; Female
                </div>
            </li>
        </ul>
    </div>
    <div id="chk" style="width: 100%; height: 35px;">
        @Html.CheckBoxFor(model => Model.IsUsingModeratorCredentials, new { id = "rbtusngModerator", @checked = Model.IsUsingModeratorCredentials })
        Login with Moderator Credentials
    </div>
    <div id="pwd" style="width: 100%">
        <ul>
            <li style="width: 100%" class="inline">
                <div class="tldiv inline">
                    <h6>
                        Mail ID</h6>
                </div>
                <div class="trdiv inline">
                    @Html.TextBoxFor(model => Model.EmailId, new { @class = "create-logtxts" })
                </div>
            </li>
            <li style="width: 100%" class="inline">
                <div class="tldiv inline">
                    <h6>
                        Password</h6>
                </div>
                <div class="trdiv inline">
                    @Html.PasswordFor(model => Model.Password, new { id = "txtPassword", @class = "create-logtxts" })
                </div>
            </li>
            <li style="width: 100%" class="inline">
                <div class="tldiv inline">
                    <h6>
                        Confirm password</h6>
                </div>
                <div class="trdiv inline">
                    <input type="password" id="txtConfirmPassword" name="ConfirmPassword" value="" class="create-logtxts" />
                </div>
            </li>
        </ul>
    </div>
    <div style="width: 100%" class="inline">
        <button id="btnCancel" style="float: right;" class="graygrad">
            Cancel</button>
        <input type="submit" id="btnSubmit" style="float: right; margin-right: 10px;" class="greengrad"
            value="Create" />
    </div>
    </form>
</div>
