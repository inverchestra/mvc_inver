<div>
    <table id="thead">
        <thead>
            <tr>
                @* <th style="width: 5%"></th>*@
                <th style="width: 16%; padding-left: 18px">
                    Date
                </th>
                <th style="width: 20%; padding-left: 18px">
                    Transaction id
                </th>
                <th style="width: 20%; padding-left: 18px">
                    Paid with
                </th>
                <th style="width: 15%; padding-left: 18px">
                    Amount
                </th>
                <th style="width: 15%; padding-left: 18px">
                    Status
                </th>
            </tr>
        </thead>
    </table>
    <table id="pra">
        <tbody>
            @foreach (var item in Model.LstOP)
            {
                <tr>
                    @*  <td style="width: 5%"></td>*@
                    <td style="width: 16%; padding-left: 18px">@item.OnlinePaymentDate
                    </td>
                    <td style="width: 20%; padding-left: 18px">
                        <label style="color: #035583; font: 14px segoe ui;">@item.TransactionId</label>
                    </td>
                    <td style="width: 20%; padding-left: 18px">
                        @item.TransactionDetailsInfo.x_card_type
                    </td>
                    <td style="width: 15%; padding-left: 18px">
                        $ @item.TransactionDetailsInfo.x_amount
                    </td>
                    <td style="width: 15%; padding-left: 18px">
                        @if (@item.TransactionDetailsInfo.x_response_code == "1")
                        {
                            <label>
                                Approved</label>                            
                        }
                        else if (@item.TransactionDetailsInfo.x_response_code == "2")
                        {
                            <label>
                                Declined</label> 
                        }
                        else if (@item.TransactionDetailsInfo.x_response_code == "3")
                        {
                            <label>
                                Error</label> 
                        }
                        else if (@item.TransactionDetailsInfo.x_response_code == "4")
                        {
                            <label>
                                Held for Review</label> 
                        }
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
