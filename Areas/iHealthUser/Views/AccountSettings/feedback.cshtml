@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.ContactUsViewModel
@{
    ViewBag.title = "";
}
<script src="../../CustomScripts/Administration.js" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        $('#btnCancel').click(function () {
            $.modal.close();

        });

    });
</script>
<style type="text/css">
    #simplemodal-container
    {
        width: 452px !important;
    }
</style>
@*<script src="../../CustomScripts/change-pswd.js" type="text/javascript"></script>*@
@Scripts.Render("~/bundles/ajaxfun")
@*<div id="AddUserDiv" class="bluegrad" style="height: 160px; width: 400px;">*@
@*<form id="Contactform" >*@
<div id="feedbackform">
    <div class="divTable1">
        <div class="tabs">
            <a href="#tab1" id="tab1" class="Current">Contact Us</a> <a href="#tab2" id="tab2"
                class="tab_static">Feedback</a>
        </div>
        <div id="Contact">
            <div id="divcontact">
                <h3>
                    Tell us about your issue
                </h3>
            </div>
            <div class="Payment">
                @using (Ajax.BeginForm("ContactUs", "AccountSettings", new AjaxOptions() { HttpMethod = "POST", OnSuccess = "successContact" }))
                {
                    @Html.ValidationSummary(true); 
                    <ul>
                        <li class="labelnormal">Email<text style="color: Red">*</text></li>
                        <li class="payment_txtbx">@Html.TextBoxFor(model => model.contactUs.EmailId, new { @placeholder = "Enter your email id", @class = "logintxts" })
                            @Html.ValidationMessageFor(model => model.contactUs.EmailId)</li>
                        <li class="labelnormal">Subject<text style="color: Red">*</text></li>
                        <li class="payment_txtbx">
                            @Html.TextBoxFor(model => model.contactUs.Subject, new { @placeholder = "Subject", @class = "logintxts" })
                            @Html.ValidationMessageFor(model => model.contactUs.Subject)</li>
                        <li class="labelnormal">Description<text style="color: Red">*</text></li>
                        <li class="payment_txtbx">
                            @Html.TextAreaFor(model => model.contactUs.Description, new { @placeholder = "Description", @class = "logintxts", style = "margin:0; width:92%; heigh:80px; border: 1px solid #BED0DF;" })
                            @Html.ValidationMessageFor(model => model.contactUs.Description)
                        </li>
                        <li class="labelnormal">&nbsp;</li>
                        <li class="payment_txtbx">
                            <div style="float: right; margin: 0 8px 0 0;">
                                <input id="btnCancel" class="graygrad" type="button" value="Close" />
                                <input id="btnSubmit1" class="greengrad" type="submit" value="Send" />
                            </div>
                        </li>
                    </ul>
                }
            </div>
        </div>
        <div id="Feedback">
            <div id="divfeedback">
                <h3>
                    Suggest us...</h3>
            </div>
            <div class="Payment">
                @using (Ajax.BeginForm("Feedback1", "AccountSettings", new AjaxOptions() { HttpMethod = "POST", OnSuccess = "successFeedback" }))
                {
                    @Html.ValidationSummary(true);

                    <ul>
                        <li class="labelnormal">Email<text style="color: Red">*</text></li>
                        <li class="payment_txtbx">
                            @Html.TextBoxFor(model => model.feedBack.Email, new { @placeholder = "Enter your email", @class = "logintxts" })
                            @Html.ValidationMessageFor(model => model.feedBack.Email)</li>
                        <li class="labelnormal">Subject<text style="color: Red">*</text></li>
                        <li class="payment_txtbx">
                            @Html.TextBoxFor(model => model.feedBack.FSubject, new { @placeholder = "Subject", @class = "logintxts" })
                            @Html.ValidationMessageFor(model => model.feedBack.FSubject)</li>
                        <li class="labelnormal">Description</li>
                        <li class="payment_txtbx">
                            @Html.TextAreaFor(model => model.feedBack.FDescription, new { @placeholder = "Description", @class = "logintxts", style = "margin:0; width:92%; heigh:100px; border: 1px solid #BED0DF;" })
                            @Html.ValidationMessageFor(model => model.feedBack.FDescription)</li>
                        <li class="labelnormal">&nbsp;</li>
                        <li class="payment_txtbx">
                            <div style="float: right; margin: 0 8px 0 0;">
                                <input type="button" id="btnfcancel" class="graygrad" value="Close" />
                                <input type="submit" id="btnfeedbacksave" class="greengrad" value="Send" />
                            </div>
                        </li>
                    </ul>
                }
            </div>
        </div>
    </div>
</div>
