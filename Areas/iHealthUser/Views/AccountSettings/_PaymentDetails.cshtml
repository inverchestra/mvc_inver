@model InnDocs.iHealth.Web.UI.Models.LoginAndSignUp

<div>
    <table id="thead">
        <thead>
            <tr>
                <th style="width: 5%">
                    Type
                </th>
                <th style="width: 25%; padding-left: 18px">
                    Payment method
                </th>
                <th style="width: 40%; padding-left: 18px">
                    Last Payment
                </th>
               @* <th style="width: 5%">
                </th>*@
            </tr>
        </thead>
    </table>
    <table id="pra">
        <tbody>
            
            @foreach( var item in Model.LstOP)
            {
                <tr>
                    <td style="width: 5%">
                        @if(@item.TransactionDetailsInfo.x_card_type=="Visa")
                        {
                        <img src="~/Images/Visacard.png" alt="Visa" style="width: 30px; height: 24px;"
                            title="Visa" />
                        }
                        else if (@item.TransactionDetailsInfo.x_card_type == "MasterCard")
                        {
                        <img src="~/Images/Mastercard.png" alt="MasterCard" style="width: 30px; height: 24px;"
                            title="Master" />
                        }
                        else if (@item.TransactionDetailsInfo.x_card_type == "Discover")
                        {
                        <img src="~/Images/DiscoverCard.png" alt="Discover" style="width: 30px; height: 24px;"
                            title="Discover" />
                        }
                        else if (@item.TransactionDetailsInfo.x_card_type == "American Express")
                        {
                        <img src="~/Images/american-express.png" alt="American Express" style="width: 30px; height: 24px;"
                            title="American Express" />
                        }
                        
                    </td>
                    <td style="width: 25%; padding-left: 18px">
                        @item.TransactionDetailsInfo.x_card_type &nbsp @item.TransactionDetailsInfo.x_account_number
                    </td>
                    <td style="width: 40%; padding-left: 18px">
                    @item.OnlinePaymentDate <label style="color:#035583; font:14px segoe ui;"> &nbsp; &nbsp;Transaction Id: @item.TransactionId</label>

                    </td>
                   @* <td style="width: 5%">
                   
                       
                    </td>*@
                </tr>
            }
        </tbody>
    </table>
</div>
