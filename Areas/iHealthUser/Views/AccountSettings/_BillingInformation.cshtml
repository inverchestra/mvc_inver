@model InnDocs.iHealth.Web.UI.Models.LoginAndSignUp
@*@Page.Value.OnlinePaymentDate.ToString()
@Model.OP.TransactionDetailsInfo.x_trans_id*@
<h4 style="color: #004469">Billing Information</h4>

<div class="PAccount" style="margin-top:0px">
    <div id="details" style="width: 45%; float: left">
        <ul style="margin-left: 10px">
            <li style="font-weight: normal; font-size: 12px; border:none;">First name</li>
            <li style="width: 99%; margin: 0 0 12px 5px; border:none;">
                <h3>@Model.OP.TransactionDetailsInfo.x_first_name</h3>
            </li>
            <li style="font-weight: normal; font-size: 12px; border:none;">Last name</li>
            <li style="width: 99%; margin: 0 0 12px 5px; border:none;">
                <h3>@Model.OP.TransactionDetailsInfo.x_last_name</h3>
            </li>
            <li style="font-weight: normal; font-size: 12px; border:none;">Company name</li>
            <li style="width: 99%; margin: 0 0 12px 5px; border:none;">
                <h3>@Model.OP.TransactionDetailsInfo.x_company</h3>
            </li>
            <li style="font-weight: normal; font-size: 12px; border:none;">Address</li>
            <li style="width: 99%; margin: 0 0 12px 5px; border:none;">
                <h3>@Model.OP.TransactionDetailsInfo.x_address</h3>
            </li>
            <li style="font-weight: normal; font-size: 12px; border:none;">City</li>
            <li style="width: 99%; margin: 0 0 12px 5px; border:none;">
                <h3>@Model.OP.TransactionDetailsInfo.x_city- @Model.OP.TransactionDetailsInfo.x_zip</h3>
            </li>
            <li style="font-weight: normal; font-size: 12px; border:none;">State</li>
            <li style="width: 99%; margin: 0 0 12px 5px; border:none;">
                <h3>@Model.OP.TransactionDetailsInfo.x_state</h3>
            </li>

            <li style="font-weight: normal; font-size: 12px; border:none;">Country</li>
            <li style="width: 99%; margin: 0 0 12px 5px; border:none;">
                <h3>@Model.OP.TransactionDetailsInfo.x_country</h3>
            </li>

        </ul>

    </div>
    <div id="contact" style="width: 27%; float: left">
        <table>
            <tbody>
                <tr>
                    <td style="font-weight: normal; font-size: 12px; padding: 2px 0 0 35px" class="PhoneMailFax_Icons Email">Email:
                    </td>
                    <td>
                        <h2>@Model.OP.TransactionDetailsInfo.x_email</h2>
                    </td>
                </tr>
                <tr>
                    <td style="font-weight: normal; font-size: 12px; padding: 2px 0 0 35px" class="PhoneMailFax_Icons Phone">Mobile:
                    </td>
                    <td>
                        <h2>@Model.OP.TransactionDetailsInfo.x_phone</h2>
                    </td>
                </tr>
                <tr>
                    <td style="font-weight: normal; font-size: 12px; padding: 2px 0 0 35px" class="PhoneMailFax_Icons Fax">Fax:
                    </td>
                    <td>
                        <h2>@Model.OP.TransactionDetailsInfo.x_fax</h2>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
  m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-47299480-1', 'bumpdocs.com');
        ga('send', 'pageview');

</script>

</div>
