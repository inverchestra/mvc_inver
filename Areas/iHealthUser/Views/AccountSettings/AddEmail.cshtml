@model InnDocs.iHealth.Web.UI.Models.UserInformation
@{
    ViewBag.Title = "Add Email";
}
<div class="bluegrad" style="width: 250px;">
    <div style="border-bottom: 1px solid rgb(218,221,226);">
       <h4 style="width:162px; margin:0 0 1px 0px;">Add email</h4 >
    </div>
    <div style="width: auto">
        <form id="frmAddEmail" action="">
        <input type="hidden" name="UserId" value="@Model.UserId" />
        <ul>
           <li><input type="text" id="txtEmail" name="EmailId" value="@Model.EmailId" placeholder="Enter valid email id"   style="width:229px; margin:0 0 15px 0" />
        </li> 
        </ul>
        <div style="width: 100%" class="inline">
            <button id="btnCancel" style="float: right;" class="graygrad" onclick="$.modal.close();">
                Cancel</button>
            <input type="submit" id="btnaddemail" style="float: right; margin-right: 10px;"
                class="greengrad" value="Save" />
        </div>

        </form>
    </div>
