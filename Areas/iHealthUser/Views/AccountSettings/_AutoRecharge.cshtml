@model InnDocs.iHealth.Web.UI.Models.LoginAndSignUp
@{
    ViewBag.Title = "Auto Recharge";
                //string dID = Model.FirstOrDefault().DomainId.ToString();
}
<script type="text/javascript">
    $(function () {
        $("#btnPayment").click(function () {
            var uId = $("#uid").val();
            // alert("btnPay clicked");
            // alert(uId);

            $.ajax({
                url: '/iHealthUser/OnlinePaymentUpdate/UpdatePayment', //Update Payment Related
                type: 'GET',
                // data: { UID: uId },
                success: function (result) {
                    //$.modal.close();
                    // alert(result);
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }
                    });
                }
            });
        });
    });


</script>

<div class="PAccount" style="margin-top:0px">
    <ul>
        <li class="labelPA" style="width: 40%;">First name </li>
        <li class="ValuePA" style="width: 60%;">
            <label id="lblFirstName">@Model.SingleRegUser.FirstName</label>
        </li>
        <li class="labelPA" style="width: 40%;">Email id </li>
        <li class="ValuePA" style="width: 60%;">
            <label id="lblEmailid">@Model.SingleRegUser.EmailId</label>
        </li>
        <li class="labelPA" style="width: 40%;">Contact number </li>
        <li class="ValuePA" style="width: 60%;">
            <label id="lblPhno">@Model.SingleRegUser.PhoneNumber</label>
        </li>
        <li class="labelPA" style="width: 40%;">User plan </li>
        <li class="ValuePA" style="width: 60%;">
            <label id="lblUsrPlan">@Model.SingleRegUser.UserPlan</label>
        </li>
        @*<li class="labelPA" style="width: 40%;">User type </li>
        <li class="ValuePA" style="width: 60%;">
            <label id="lblUsrType">Single user</label>
        </li>*@
