@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.MyDocsTree
@{
    ViewBag.Title = "mydocs";
}
<script type="text/javascript">
    $(document).ready(function () {

        $.ajaxSetup({ async: false, cache: false });

        $("#searchBtn").click(function () {
            var searchtext1 = $('input[name="searchtext1"]').val();
//            alert("searchtext1" + searchtext1);
            $.ajax({
                url: '/ihealthuser/HealthDocs/GetSearchedDocs',
                data: { searchtext1: searchtext1 },
                dataType: 'html',
                type: 'GET',
                success: function (data) {
                    // alert(data);
                    // $("#TreeViewData").html(data[0].OriginalName);
                    $("#TreeViewData").html(data);
                }
            });
            return false;
        });


    });
</script>
<div id="search" style="float: right">
    @* <form id="searchform" method="post" action="@Url.Action("GetSearchedDocs", "HealthDocs", new { pId = 0 })">*@
    <div style="float: left; margin-right: -50px;">
        <input type="text" name="searchtext1" />
    </div>
    <div style="float: right;">
        <input id="searchBtn" type="submit" value="Search" style="background-image: url(~/Images/searchiconT.png);
            display: inline-block;" />
    </div>
    @* </form>*@
</div>
<div>
    <div>
        @RenderPage("~/Areas/iHealthUser/Views/Shared/TreeViewCases.cshtml", new { Values = Model })
    </div>
</div>
