@section LeftPanel {
    <div id="leftside-wrapper">
        <div class="leftSide_bar_wrapper">
            <ul>
                <li><a href="@Url.Action("DoulaVitals", "DoulaMedwall")">Vitals</a></li>
                <li><a href="@Url.Action("Index", "DoulaMedwall")" >Timeline</a></li>
                <li><a href="@Url.Action("GetMonths", "DoulaMedwall")">
                    <label style="color: #CA2D70;">
                        Albums</label></a></li>
                      <li><a href="@Url.Action("DoulaPInfo", "DoulaPersonal")" >My<span style="color: #fff">-</span>Info</a></li>
                @if (ViewBag.statustip == "0")
                {
                    <li><span id="badge-page4"></span><a href="@Url.Action("ShowAllTips", "DoulaMedwall")">Tips</a></li>
                }
                else
                {
                    <li><span id="badge-page2">@ViewBag.statustip</span><a href="@Url.Action("ShowAllTips", "DoulaMedwall")">Tips</a></li>
                }
                @if (ViewBag.statusresponce == "0")
                {
                    <li><span id="badge-page3"></span><a href="@Url.Action("ShowAllQuestions", "DoulaMedwall")">
                        Questions</a></li>
                }
                else
                {
                    <li><span id="badge-page1">@ViewBag.statusresponce</span><a href="@Url.Action("ShowAllQuestions", "DoulaMedwall")">
                        Questions</a></li>
                }
            </ul>
        </div>
    </div>
}
