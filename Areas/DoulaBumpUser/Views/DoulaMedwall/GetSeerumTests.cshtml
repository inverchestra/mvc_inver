@model IEnumerable<InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.SerumTest>
<script type="text/javascript">
    $(function () {
        $('input[name="Serumtest"]').click(function () {
            var srid = $(this).siblings(':hidden').val();
            // alert(srid);
            //  alert(srid);
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/SerumTestView',
                type: 'GET',
                data: { srid: srid },
                success: function (result) {
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        //position: ["2%", "30%"],
                        closeHTML: "",
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }

                    });
                }
            });
        });
    });
</script>
<li id="proctable">
    
    <ul class="li_color" style="margin:0px">
    @if (Model.Count() > 0)
    {
        foreach (var value in Model)
        {
            
            <li style="float:right; display: inline-block">
        @value.CreatedOn

      
            <input type="hidden" name="esname" value="@value.SerumId" />
            <input type="button" id="Serumtest" name="Serumtest" class="viewbutton" value="View" />
        </li>                          
        }

    }
    else
    {
        <text>No tests</text>
    }
        </ul>
</div>
<div id="tab" style="display: none">
</div>
