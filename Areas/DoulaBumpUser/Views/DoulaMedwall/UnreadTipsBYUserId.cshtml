@model IEnumerable<InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.Tips>
@{
    ViewBag.title = "";
}
@if (Model != null && Model.Count() > 0)
{
    <ul id="update1">
        @foreach (var tips in Model)
        {
            <li class="listyle" style="width: 96%; color: #585858; font-family: 'tauri' sans-serif;">
               <label style="color: rgb(202, 45, 112); font-size: medium">
                    @tips.Day tips: 
                </label>

                 @tips.Tip

                @if (tips.Status == true)
                {

                    <img src="../../../../Images/read.png" alt="readmsg" height="32" width="32" style="float: right" />

                }
                else
                {
                    <a href="#">
                        <img src="../../../../Images/unread-1.png" alt="readmsg" height="32" width="32" style="float:right" />
                    </a>
                }
            </li>


        }

    </ul>
}
else
{
    <h3 style="color:gray; font-size:xx-large; text-align:center; margin-top:100px">No Unread tips !</h3>
}

