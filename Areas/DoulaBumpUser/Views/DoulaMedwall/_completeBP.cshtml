@using System.Web.Optimization
@using InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals
@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.Vitals.Vitals
@{       
    CompleteBloodPicture obj = new CompleteBloodPicture();
    List<SelectListItem> noOfBloodTests = obj.NoOfBloodTests();
   
}
<script type="text/javascript">
    $(function () {
        $('#btnAddBloodInfo').click(function () {
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/CompleteBPEdit',
                type: 'GET',
                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }
                    });
                }
            });
        });

        $('#btnAddBloodTest1').click(function () {
            var bloodTestValue = $('#btValue').val();
            var bloodTestName = $('.Reg_Details_select1 option:selected').text();
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/AddBloodTestPopup',
                type: 'POST',
                async: false,
                cache: false,
                data: { bloodTestName: bloodTestName, bloodTestValue: bloodTestValue },

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }
                    });
                }
            });
        });

        $('a.BloodTestsTrend').click(function () {
            var BTsName = $(this).attr('id');

            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/BloodTestsTrend',
                type: 'GET',
                async: false,
                cache: false,
                data: { BTsName: BTsName },
                success: function (ResultData) {
                    $('#ajaxtab').html(ResultData).modal({

                        escClose: true,
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto' });
                        }
                    });
                }
            });
        });

        //$(document).ready(function () {
        //    $('#tab-body').css({ 'box-shadow': '0 0 20px 0px #d5d5d5 inset',});
        //});
    });
</script>

