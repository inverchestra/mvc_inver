@{
    ViewBag.Title = "";
}

<link href="~/Content/resolution.css" rel="stylesheet" />

<script type="text/javascript">
    $(function () {


        $('div#ErlyScan').load('/DoulaBumpUser/DoulaMedwall/EarlyScans');
        $('div#NtScan').load('/DoulaBumpUser/DoulaMedwall/NtScans');
        $('div#AnlyScans').load('/DoulaBumpUser/DoulaMedwall/AnomalyScans');
        $('div#GrthScan').load('/DoulaBumpUser/DoulaMedwall/GrowthScans');

        $('#btnAddEarly').click(function () {
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/EarlyScan',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        position: ["2%", "28%"],
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'top': '0px !important' });
                        }

                    });
                }
            });
        });
        $('#btnAddNT').click(function () {
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/NTScan',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        position: ["2%", "28%"],
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'top': '0px !important' });
                        }
                    });
                }
            });
        });
        $('#btnAddAnly').click(function () {
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/AnamolyScan',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        position: ["2%", "28%"],
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'top': '0px !important' });
                        }
                    });
                }
            });
        });
        $('#btnAddGrth').click(function () {
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/GrowthScan',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        position: ["2%", "28%"],
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'margin-top': '0px !important' });
                        }
                    });
                }
            });
        });


        $('#btnNewData').click(function () {
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/UstAdd',
                type: 'GET',

                success: function (result) {
                    $.modal.close();
                    $('#ajaxtab').html(result).modal({
                        escClose: true,
                        closeHTML: "",
                        position: ["2%", "28%"],
                        onShow: function (e) {
                            e.container.css({ 'height': 'auto', 'width': 'auto', 'top': '0px !important' });

                        }

                    });

                }
            });
        });

        $("#btnUstTrend").click(function () {
            $.ajax({
                url: '/DoulaBumpUser/DoulaMedwall/UstTrend',
                type: 'GET',
                async: false,
                cache: false,
                success: function (ResultData) {
                    // alert("1");
                    // alert(ResultData);
                    $('#ajaxtab').html(ResultData).modal({

                        escClose: true,
                        position: ["2%", "28%"],
                        // closeHTML: "",
                        onShow: function (e) {

                            e.container.css({ 'height': 'auto', 'width': 'auto', 'top': '0px !important' });

                        }

                    });

                }
            });
        });



    });
</script>
<div class="vitals">
    <ul>
        <li style="">
            <label style="width: 100%; display: inline-block; background-color: #f7f7f7; font-weight: bold; color: gray">Early Scans</label>
            <div id="ErlyScan" class="vitals_add">
            </div>
            <div class="vitals_view">
               @* <input id="btnAddEarly" type="button" class="addbutton" value="Add Scan" />*@
            </div>
        </li>
        <li style="">
            <label style="width: 100%; display: inline-block; background-color: #f7f7f7; font-weight: bold; color: gray">NT Scans</label>
            <div id="NtScan" class="vitals_add">
            </div>
            <div class="vitals_view">
               @* <input id="btnAddNT" type="button" class="addbutton" value="Add Scan" />*@
            </div>
        </li>
        <li style="">
            <label style="width: 100%; display: inline-block; background-color: #f7f7f7; font-weight: bold; color: gray">Anomaly Scans</label>
            <div id="AnlyScans" class="vitals_add">
            </div>
            <div class="vitals_view">
               @* <input id="btnAddAnly" type="button" class="addbutton" value="Add Scan" />*@
            </div>
        </li>
        <li style="">
            <label style="width: 100%; display: inline-block; background-color: #f7f7f7; font-weight: bold; color: gray">Growth Scans</label>
            <div id="GrthScan" class="vitals_add">
            </div>
            <div class="vitals_view">
               @* <input id="btnAddGrth" type="button" class="addbutton" value="Add Scan" />*@
