@model InnDocs.iHealth.Web.UI.Models.UserInformation


<div id="pay">
    <h1>Continue to pay</h1>
    <p>Click to follow with payment process...</p>
    <p>Monthly pay - <span>$10</span></p>

    <form id="simForm" method='post' action='https://test.authorize.net/gateway/transact.dll'>

        <input type='hidden'  name='x_login' id='x_login' value='@Model.x_login '  />
        <input type='hidden'  name='x_amount' id='x_amount' value='@Model.x_amount' />
        <input type='hidden'  name='x_cust_id' id='x_cust_id' value='@Model.x_cust_id' />
        <input type='hidden'  name='x_description' id='x_description' value='@Model.x_description' />
        <input type='hidden'  name='x_invoice_num' id='x_invoice_num' value='@Model.x_invoice_num' />
        <input type='hidden'  name='x_fp_sequence' id='x_fp_sequence' value='@Model.x_fp_sequence' />
        <input type='hidden'  name='x_fp_timestamp' id='x_fp_timestamp' value='@Model.x_fp_timestamp' />
        <input type='hidden'  name='x_fp_hash' id='x_fp_hash' value='@Model.x_fp_hash' />
        <input type='hidden' name='x_test_request' id='x_test_request' value='@Model.x_test_request' />

        <input type='hidden' name='x_show_form' value='PAYMENT_FORM' />
        <input type='submit' value="PAY NOW" id='buttonLabel' />

    </form>
</div>
