@using System.Web.Optimization
@using InnDocs.iHealth.Web.UI.Models
@model InnDocs.iHealth.Web.UI.Areas.iHealthUser.Models.UserRepository.ContactUsViewModel
@using System.Web.Optimization;
@{
    ViewBag.Title = "new feedbacjk";
}
<script src="../../CustomScripts/Administration.js" type="text/javascript"></script>
<script src="../../CustomScripts/change-pswd.js" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        $('#btnCancel').click(function () {
            $.modal.close();

        });

    });
</script>
@Scripts.Render("~/bundles/ajaxfns")
<div id="feedbackform">
    <div class="divTable1">
        <div class="tabs">
            <a href="#tab1" id="tab1" class="Current">Contact Us</a> <a href="#tab2" id="tab2"
                class="Current">Feedback</a>
        </div>
        <div id="Contact">
            <div id="divcontact">
                <h3>
                Tell us about your issue
                </h3>
            </div>
          
            <div>
                @using (Ajax.BeginForm("ContactUs", "Home", new AjaxOptions() { HttpMethod = "POST", OnSuccess = "closeModal", OnFailure = "displayError", OnBegin = "loading" }, new { }))
                {
                    @Html.ValidationSummary(true);


                    @Html.TextBoxFor(model => model.contactUs.EmailId, new { @placeholder = "Enter your Email", @class = "logintxts" })
                    @Html.ValidationMessageFor(model => model.contactUs.EmailId)
                    @Html.TextBoxFor(model => model.contactUs.Subject, new { @placeholder = "Subject", @class = "logintxts" })
                    @Html.ValidationMessageFor(model => model.contactUs.Subject)
                    @Html.TextAreaFor(model => model.contactUs.Description, new { @placeholder = "Description", @class = "logintxts" })
                    @Html.ValidationMessageFor(model => model.contactUs.Description)




 
                    <div style="float: right">
                        <input id="btnSubmit1" class="greengrad" type="submit" value="Send" /> 
                    <input id="btnCancel" class="greengrad" type="button" value="Close" />
                       
                    </div>


                }
            </div>
        </div>
        <div id="Feedback">
            <div id="divfeedback">
                <h3>
                    Suggest us...
                </h3>
            </div>           
            <div>
                @using (Ajax.BeginForm("Feedback1", "Home", new AjaxOptions() { HttpMethod = "POST", OnSuccess = "closeModal", OnFailure = "displayError", OnBegin = "loading" }))
                {
                    @Html.ValidationSummary(true);
                    @Html.TextBoxFor(model => model.feedBack.Email, new { @placeholder = "Enter your Email", @class = "logintxts" })
                    @Html.ValidationMessageFor(model => model.feedBack.Email)
                    @Html.TextBoxFor(model => model.feedBack.FSubject, new { @placeholder = "Subject", @class = "logintxts" })
                    @Html.ValidationMessageFor(model => model.feedBack.FSubject)
                    @Html.TextAreaFor(model => model.feedBack.FDescription, new { @placeholder = "Description", @class = "logintxts" })
                    @Html.ValidationMessageFor(model => model.feedBack.FDescription)

                    <div style="float: right">
                        <input type="button" id="btnfcancel" class="greengrad" value="Close" />
                        <input type="submit" id="btnfeedbacksave" class="greengrad" value="Send" />
                    </div>
                }
            </div>
        </div>
    </div>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
  m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-47299480-1', 'bumpdocs.com');
        ga('send', 'pageview');

</script>
